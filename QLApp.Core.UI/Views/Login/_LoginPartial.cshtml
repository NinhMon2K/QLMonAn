@{
    Layout = null;

}

<link href="~/lib/themify-icons/themify-icons.css" rel="stylesheet" />
<link href="~/css/view/dictionary/table.css" rel="stylesheet" />
<form class="page-view login-partial">
    <link href="~/css/view/login/_LoginPartial.css" rel="stylesheet" />
    <div class="login-page">
        <div class="login-container">
            <div class="login-content">
                <div class="top-login">
                    <div class="logo-phone">

                    </div>
                    <div class="name-phone">Quản lý giới thiệu món ăn</div>
                </div>
                <div class="input-field input-margin">
                    <div class="text-box">
                        <input type="text" name="name" id="username" placeholder="Tài khoản" value="" />
                    </div>
                </div>
                <div class="input-field input-margin">
                    <div class="text-box user-password">


                        <input type="password" class="text-password" id="password" name="name" placeholder="Mật khẩu" value="" />

                        <div class="child-password">
                            <i class="ti-eye"></i>
                        </div>
                    </div>
                </div>
                <div class="button-login">

                    <div class="btn-Accept">
                        <button type="button" id="btn-login" class="btn btn-login"><a href="#" >Đăng nhập</a></button>
                    </div>
                    <div class="bottom-login">
                        <div class="login-lefts"><a class="registration-user" id="registration-user" href="#">Đăng kí</a></div>
                        <div class="login-rights"><a class="familiar-user" href="#">Quên mật khẩu</a></div>
                    </div>

                </div>
            </div>

        </div>


    </div>


</form>
<div id="dialog-form" title="Đăng kí">
    <form id="">

        <div class="page-content">
            <div class="page_row page_row-1">
                <div class="box box_tk">
                    <div class="box_left">
                        <label for="name">Tài khoản:</label>
                    </div>
                    <div class="box_right">
                        <input type="text" name="taikhoan" id="username_cr"  class="text ui-widget-content ui-corner-all">
                    </div>
                </div>

                <div class="box box_mk">
                    <div class="box_left">
                        <label for="email">Mật khẩu:</label>

                    </div>
                    <div class="box_right">
                        <input type="text" name="matkhau" id="password_cr"  class="text ui-widget-content ui-corner-all">
                    </div>
                </div>
                <div class="box box_ht">
                    <div class="box_left">
                        <label for="password">Họ tên:</label>

                    </div>
                    <div class="box_right">
                        <input type="text" name="password" id="fullname"  class="text ui-widget-content ui-corner-all">
                    </div>
                </div>
                <div class="box box_ht">
                    <div class="box_left">
                        <label for="password">Giới tính:</label>

                    </div>
                    <div class="box_right box_gt">
                        <div class="gt gt_nam">
                            <input type="radio" id="rdonam" name="nam" value="nam">
                            <label>Nam</label>
                        </div>
                        <div class="gt gt_nu">
                            <input type="radio" id="rdonu" name="nam" value="nam">
                            <label>Nữ</label>
                        </div>

                    </div>
                </div>
                <div class="box box_ht">
                    <div class="box_left">
                        <label>Ngày sinh:</label>
                    </div>
                    <div class="box_right">
                        <input type="date" id="ngaysinh" name="date">
                    </div>
                </div>
            </div>
            <div class="page_row page_row-2">
                <div class="box box_sdt">
                    <div class="box_left">

                        <label for="name">Số điện thoại:</label>

                    </div>
                    <div class="box_right">
                        <input type="text" name="" id="sdt"  class="text ui-widget-content ui-corner-all">
                    </div>
                </div>
                <div class="box box_email">
                    <div class="box_left">

                        <label for="name">Email:</label>

                    </div>
                    <div class="box_right">
                        <input type="text" name="" id="email"  class="text ui-widget-content ui-corner-all">
                    </div>
                </div>
                <div class="box box_tk">
                    <div class="box_left">

                        <label for="name">Hình ảnh:</label>

                    </div>
                    <div class="box_right">
                        <input type="file" name="" id="hinhanh"  class="text ui-widget-content ui-corner-all">
                    </div>
                </div>
                <div class="box box_tk">
                    <div class="box_left">

                        <label for="name">Địa chỉ:</label>

                    </div>
                    <div class="box_right">
                        <input type="text" name="" id="diachi"  class="text ui-widget-content ui-corner-all">
                    </div>
                </div>

            </div>

        </div>
        <div class="page-footer">
            <div class="footer-content" style="display:flex">
                <div class="left-footer btn_left">
                    <button type="button" id="btn_close" class="btn">Đóng</button>
                </div>
                <div class="right-footer">
                    <button type="button" class="btn" id="btn_add">Lưu</button>
                </div>
            </div>
        </div>
    </form>
</div>

<script src="~/js/view/login/_LoginPartial.js"></script>
